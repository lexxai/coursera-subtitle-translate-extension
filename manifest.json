{
  "manifest_version": 3,
  "name": "__MSG_plug_name__",
  "description": "__MSG_plug_description__",
  "version": "1.4.4",
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://*.coursera.org/*"],
  "content_scripts": [
    {
      "matches": ["*://www.coursera.org/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"],
  "default_locale": "en",
  "icons": {
    "16": "/images/cst-16x16.png",
    "32": "/images/cst-32x32.png",
    "64": "/images/cst-64x64.png",
    "128": "/images/cst-128x128.png"
  }
}
