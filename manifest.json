{
  "manifest_version": 2,
  "name": "Coursera Subtitle Translation",
  "description": "Coursera Subtitle Translation",
  "version": "1.1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.coursera.org/*"],
      "js": ["background.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs", "storage", "activeTab"]
}
